        <main class="contact">
                <h2 class="contact__title">Contact us</h2>
                <hr>
                <form class="contact__form" action="" method="">
                        <div class="contact__form-box">
                                <label for="">Your name</label>
                                <input type="text">
                        </div>
                        <div class="contact__form-box">
                                <label for="">Your email</label>
                                <input type="email">
                        </div>
                        <div class="contact__form-box">
                                <label for="">Topic</label>
                                <select name="" id="">
                                        <option value="" disabled>--Please choose a topic--</option>
                                        <option value="topic2">topic2</option>
                                        <option value="topic3">topic3</option>
                                        <option value="topic4">topic4</option>
                                        <option value="other">other topic</option>
                                </select>
                        </div>
                        <div class="contact__form-box">
                                <label for="">Your message</label>
                                <textarea name="" id=""></textarea>
                        </div>
                        <button class="contact__form-btn" type="submit">Send</button>

                </form>
        </main>

        <!-- JS - MAIN (localstorage & sidebar & nav)-->
        <script src="./dist/js/index.min.js"></script>
